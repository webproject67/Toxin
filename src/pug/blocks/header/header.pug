include ../logo/logo
include ../nav/nav

mixin header(options)
  header.page-header 
    .page-header__wrapper
      +logotype({})
      +nav(options)
      button.page-header__hamburger
        span(class="page-header__streak page-header__streak--top")
        span(class="page-header__streak page-header__streak--middle")
        span(class="page-header__streak page-header__streak--bottom")